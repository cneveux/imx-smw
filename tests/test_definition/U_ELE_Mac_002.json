{
	"keys": {
		"key 1": {
			"type": "AES",
			"security_size": 256,
			"policy":{
				"sign_message": [
					[
						"CMAC"
					]
				],
				"verify_message": [
					[
						"CMAC"
					]
				]
			}
		}
	},
	"subtest 1": {
		"command": "GENERATE",
		"subsystem": "ELE",
		"key_name": "key 1"
	},
	"subtest 2": { // Get the MAC size - Output buffer is NULL
		"command": "MAC_COMPUTE",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"depends": 1,
		"result": "SMW_STATUS_OK"
	},
	"subtest 3": { // Buffer too short
		"command": "MAC_COMPUTE",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"output": [
			2,
			"012345"
		],
		"depends": 1,
		"result": "SMW_STATUS_OUTPUT_TOO_SHORT"
	},
	"subtest 4": {
		"command": "MAC_COMPUTE",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"output": 16,
		"mac_id": 1,
		"depends": 1
	},
	"subtest 5": {
		"command": "MAC_COMPUTE",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": 0,
		"output": 16,
		"mac_id": 2,
		"depends": 1
	},
	"subtest 6": { // MAC length truncated but key not valid
		"command": "MAC_COMPUTE",
		"algo": "CMAC_TRUNCATED",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"output": 8,
		"depends": 1,
		"result": "SMW_STATUS_KEY_INVALID"
	},
	"subtest 7": {
		"command": "MAC_VERIFY",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"mac_id": 1,
		"depends": 4
	},
	"subtest 8": { // Verify signature invalid
		"command": "MAC_VERIFY",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"bad1bee22e409f96e93d7e117393172a"
		],
		"mac_id": 1,
		"depends": 4,
		"result": "SMW_STATUS_SIGNATURE_INVALID"
	},
	"subtest 9": {
		"command": "MAC_VERIFY",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": 0,
		"mac_id": 2,
		"depends": 5
	},
	"subtest 10": { // MAC length set to minimal but key not valid
		"command": "MAC_VERIFY",
		"algo": "CMAC",
		"key_name": "key 1",
		"input": [
			"6bc1bee22e409f96e93d7e117393172a"
		],
		"mac": [
			"0123456789ABCEFF"
		],
		"depends": 1,
		"result": "SMW_STATUS_SIGNATURE_INVALID"
	},
	"subtest 11": {
		"command": "DELETE",
		"key_name": "key 1",
		"depends": 1
	}
}